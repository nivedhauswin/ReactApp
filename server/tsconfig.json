{
  "compilerOptions": {
    "target": "es5",
    "outDir": "dist",
    "baseUrl": "src",
    "alwaysStrict": true,
    "noImplicitAny": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop":true,
    "jsx": "react-jsx",
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
